    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ivancardenasm.simpletodolist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.ivancardenasm.simpletodolist"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "62231c13-37df-4eb3-9ae0-acc66fe3fe7e"
      }
    }
  }
}
